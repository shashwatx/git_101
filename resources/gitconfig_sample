    incommit  = "!f() { git diff "$1"^.."$1"; }; f"
    phew  = "!f() { git add -u;git commit -m "stuff";git push origin master; }; f"
    lop = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %C(blue)(%an <%ae>)%Creset' --abbrev-commit --date=relative
    lop5 = log -5 --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset %C(blue)(%an <%ae>)%Creset' --abbrev-commit --date=relative
    dsf = "!git diff --color $@ | diff-so-fancy"
